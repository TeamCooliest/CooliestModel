FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 290;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 290;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 290;
        value           uniform 290;
    }

    upperWall
    {
        type            fixedValue;
        value           uniform 290;
    }
    lowerWall
    {
        type            zeroGradient;
    }
    heatedWall
    {
        type            externalWallHeatFluxTemperature;
        mode            flux;
        q               1;
        kappaMethod     fluidThermo;
        value           $internalField;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
